{
  "expo": {
    "name": "TownDrop",
    "slug": "towndrop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/towndrop.jpg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/towndrop.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/towndrop.jpg",
        "backgroundColor": "#1a1a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mudassirms.frontend",
      "userInterfaceStyle": "dark"
    },
    "web": {
      "favicon": "./assets/towndrop.jpg"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d1c99681-8b46-4840-bd84-3ce09eae963d"
      }
    },
    "owner": "maddytown"
  }
}
